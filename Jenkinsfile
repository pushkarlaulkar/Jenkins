pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                bat 'echo %date% - %time%'
            }
        }
        stage('Hello2') {
            steps {
                echo 'Hello World2'
                bat 'echo %date% - %time%'
            }
        }
        stage('Hello3') {
            steps {
                echo 'Hello World3'
                bat 'echo %date% - %time%'
            }
        }
        stage('Hello4') {
            steps {
                echo 'Hello World4'
                bat 'echo %date% - %time%'
            }
        }
    }
}
